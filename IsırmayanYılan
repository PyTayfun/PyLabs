sifrelistesi = ["123456","password","12345678","qwerty","123456789","12345","mavielma","1234","111111","1234567","dragon","123123","baseball","abc123","football","monkey","letmein","696969","shadow","master","qwertyuiop"]

for deneme in sifrelistesi:

    if deneme == "mavielma":
        break
    print("Başarısız deneme:", deneme, "İşlem devam ediyor.")

print("Şifre çözüldü.Bulunan şifre:" , deneme)
